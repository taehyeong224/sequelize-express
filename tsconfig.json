{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "resolveJsonModule": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "baseUrl": ".",
  },
  "include": ["src/**/*"],
}